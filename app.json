{
  "expo": {
    "name": "Tremedy",
    "slug": "Tremedy",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B2A41"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#D7D9D7",
          "sounds": []
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thomas.tremedy",
      "infoPlist": {
        "NSCameraUsageDescription": "Tremedy uses quiet notifcations to remind you of Check-Ins."
      }  
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4f099d5d-0bcc-459d-b31a-9390cfa07fcd"
      }
    }
  }
}
